import base64, lzma, sys, os

if len(sys.argv) < 2:
    print("[!] Usage: python fox.txt key.txt [user:pass@host]")
    sys.exit(1)

_key_arg = sys.argv[1]

# sys.argv[1] может быть путём к key.txt или самим base64-ключом
if os.path.isfile(_key_arg):
    with open(_key_arg, "r") as _f:
        _key_b64 = _f.read().strip()
else:
    _key_b64 = _key_arg

try:
    _key = base64.b64decode(_key_b64)
except Exception:
    print("[!] Invalid key")
    sys.exit(1)

